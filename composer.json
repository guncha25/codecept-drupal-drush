    "homepage": "https://github.com/guncha25/codecept-drupal-drush",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Rigby",
            "email": "andy.rigby@ixis.co.uk"
        },
        {
            "name": "Guntis Jakovins",
